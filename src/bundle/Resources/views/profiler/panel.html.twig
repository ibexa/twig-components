{% if collector.renderedComponents is not empty %}

    {% if collector.availableGroups is not empty %}
        <h3>Available Groups</h3>
        <table>
            <thead>
            <tr>
                <th>Group</th>
            </tr>
            </thead>
            <tbody>
            {% for group in collector.availableGroups %}
                <tr>
                    <td>{{ group.group }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% endif %}

    <h3>Rendered Twig Components</h3>
    <table>
        <thead>
        <tr>
            <th>Group</th>
            <th>Component</th>
        </tr>
        </thead>
        <tbody>
        {% for component in collector.renderedComponents %}
            <tr>
                <td>{{ component.group }}</td>
                <td>{{ component.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% else %}
    <p>No Twig components were rendered.</p>
{% endif %}


